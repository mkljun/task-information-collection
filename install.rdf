<?xml version="1.0" encoding="utf-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
  
    <em:id>tic@menola.org</em:id>
    <em:name>Task Information Collections</em:name>
    <em:type>2</em:type>
    <em:version>v20130401</em:version>
    <em:description>Project/task management of information on a spatial area by creating links to files, folders, web pages, web based email and pieces of text by dragging them over the extension page.
    </em:description>
    <em:creator>mkljun</em:creator>
    <!-- optional items -->
    <em:contributor>Tine Mezgec</em:contributor>
    <em:contributor>Alan Dix</em:contributor>
    <em:homepageURL>http://pim.famnit.upr.si/tic/</em:homepageURL>
    <em:iconURL>chrome://tic/skin/images/TIC32.png</em:iconURL>
    <!-- em:aboutURL>chrome://tic/content/about.xul</em:aboutURL -->
    <em:optionsURL>chrome://tic/content/options.xul</em:optionsURL>
    <!-- type 2 means inline options -->
    <!-- em:optionsType>2</em:optionsType -->
 
    <!-- Firefox --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>13.*</em:maxVersion>
      </Description>
    </em:targetApplication>


    <!-- Any suitably modern toolkit application -->
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>13.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>

